config_version=5

[application]

config/name="FurballFusion"
config/version="v.0.0.1"
run/main_scene="res://Scene/Main.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://Asset/Image/Icons/icon.png"
config/macos_native_icon="res://Asset/Image/Icons/icon.icns"
config/windows_native_icon="res://Asset/Image/Icons/icon.ico"

[audio]

buses/default_bus_layout="res://Asset/default_bus_layout.tres"

[autoload]

Global="*res://Script/global.gd"
MusicPlayer="*res://Scene/MusicPlayer.tscn"

[display]

window/size/viewport_width=360
window/size/viewport_height=640
window/stretch/mode="viewport"
window/stretch/scale=0.5
window/handheld/orientation=1

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

